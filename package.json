{
  "name": "@tzstamp/tezos-merkle",
  "version": "0.1.0",
  "description": "Tezos-style Merkle trees",
  "main": "dist/mod.js",
  "types": "dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "deno lint src/* tests/*",
    "fmt": "deno fmt README.md CHANGELOG.md src/* tests/*",
