{
  "name": "@tzstamp/helpers",
  "version": "0.2.0",
  "description": "tzstamp helper functions",
  "main": "dist/mod.js",
  "types": "dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "deno lint --unstable src/* tests/*",
    "postlint": "deno fmt README.md CHANGELOG.md src/* tests/*",
    "build": "npx deno2node src/tsconfig.json",
    "pretest": "rm -f coverage/*",
    "test": "deno test --unstable --coverage=coverage tests/",
    "posttest": "deno coverage --unstable coverage/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tzstamp/helpers.git"
  },
  "contributors": [
    "John David Pressman <jd@jdpressman.com> (https://jdpressman.com)",
    "Benjamin Herman <benjamin@metanomial.com> (https://metanomial.com)"
  ],
  "bugs": {
    "url": "https://gitlab.com/tzstamp/helpers/issues"
  },
  "homepage": "https://gitlab.com/tzstamp/helpers",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^15.0.1"
  }
}
