{
  "name": "@tzstamp/proof",
  "version": "0.1.0",
  "description": "Cryptographic proofs for TzStamp notaries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "deno lint --unstable src/* tests/*",
    "postlint": "deno fmt README.md CHANGELOG.md src/* tests/*",
    "build": "npx deno2node src/tsconfig.json",
    "pretest": "rm -f coverage/*",
    "test": "deno test --unstable --allow-net --coverage=coverage tests/",
    "posttest": "deno coverage --unstable coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tzstamp/proof.git"
  },
  "homepage": "https://gitlab.com/tzstamp/proof",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/node-fetch": "^2.5.8"
  },
  "dependencies": {
    "@tzstamp/helpers": "^0.1.1",
    "node-fetch": "^2.6.1"
  }
}
