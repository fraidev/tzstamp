{
  "name": "@tzstamp/proof",
  "version": "0.2.0",
  "description": "Cryptographic proofs for TzStamp tools",
  "main": "dist/mod.js",
  "types": "dist/mod.d.ts",
  "scripts": {
    "lint": "deno lint --unstable src/* tests/*",
    "fmt": "deno fmt README.md CHANGELOG.md src/* tests/*",
    "build": "npx deno2node src/tsconfig.json",
    "test": "deno test --unstable --allow-net tests/",
    "coverage": "rm -f coverage/* && deno test --unstable --allow-net --coverage=coverage tests/ && deno coverage coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tzstamp/proof.git"
  },
  "contributors": [
    "John David Pressman <jd@jdpressman.com> (https://jdpressman.com)",
    "Benjamin Herman <benjamin@metanomial.com> (https://metanomial.com)"
  ],
  "homepage": "https://gitlab.com/tzstamp/proof",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/node-fetch": "^2.5.8"
  },
  "dependencies": {
    "@tzstamp/helpers": "^0.2.0",
    "jtd": "^0.1.1",
    "node-fetch": "^2.6.1"
  }
}
